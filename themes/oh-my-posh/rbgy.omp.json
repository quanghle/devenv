{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{.Folder}}{{if .Root}} :: root{{end}} :: {{.Shell}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "p:white",
          "template": "{{.Icon}}{{ if .Segments.session.SSHSession }} \uf817{{end}} "
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "p:black",
          "background": "p:yellow",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "<b>{{ .UserName }}</b><p:gray>@{{ .HostName }}</>"
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "p:white",
          "background": "transparent",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "..üìÇ..",
            "home_icon": "üè†",
            "max_depth": 2,
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "foreground": "p:lightPurple",
          "background": "p:darkPurple",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<p:red> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<p:green> \uf046 {{ .Staging.String }}</>{{ end }}",
          "properties": {
            "branch_ahead_icon": "\u2934 ",
            "branch_behind_icon": "\u2935 ",
            "branch_gone_icon": "\u2716",
            "branch_icon": "\ue725 ",
            "branch_identical_icon": "\u2714",
            "fetch_bare_info": true,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0c7",
          "invert_powerline": true,
          "foreground": "p:white",
          "background": "#4d2dcc",
          "template": " \ue77f {{ .Full }} "
        },
        {
          "type": "executiontime",
          "style": "accordion",
          "powerline_symbol": "\ue0c7",
          "invert_powerline": true,
          "foreground": "p:green",
          "background": "p:darkBlue",
          "template": " \u231b {{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true,
            "style": "austin"
          }
        },
        {
          "type": "time",
          "style": "accordion",
          "powerline_symbol": "\ue0c6",
          "foreground": "p:lightPurple",
          "background": "p:orange",
          "template": " \u23f0 {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "project",
          "style": "plain",
          "foreground": "p:gray",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\uf487 {{.Version}}{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "template": "üëë"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:yellow",
          "template": "<p:blue>\uf105</><p:red>\uf105</><p:green>\uf105</>\uf105"
        }
      ],
      "newline": true
    }
  ],
  "palette": {
    "black": "#36454F",
    "blue": "#0000ff",
    "darkBlue": "#0b2c52",
    "darkPurple": "#24014d",
    "gray": "#A9A9A9",
    "green": "#7CFC00",
    "lightPurple": "#C3B1E1",
    "orange": "#E35335",
    "red": "#E0115F",
    "white": "#E5E4E2",
    "yellow": "#FAFA33"
  }
}